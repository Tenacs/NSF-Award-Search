{
  "name": "next-flask",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "flask-dev:unix": "cross-env FLASK_DEBUG=1 ./venv/bin/pip install -r requirements.txt && ./venv/bin/python -m flask --app api/index run -p 5328",
    "flask-dev:win": "cross-env FLASK_DEBUG=1 venv\\Scripts\\pip install -r requirements.txt && venv\\Scripts\\python -m flask --app api/index run -p 5328",
    "flask-dev": "run-s flask-dev:win || run-s flask-dev:unix",
    "next-dev": "next dev",
    "dev": "concurrently \"pnpm run next-dev\" \"pnpm run flask-dev\"",
    "build": "next build",
    "start": "next start",
    "lint": "next lint"
  },
  "dependencies": {
    "@types/node": "20.1.4",
    "@types/react": "18.2.6",
    "@types/react-dom": "18.2.4",
    "autoprefixer": "10.4.14",
    "concurrently": "^9.0.1",
    "eslint": "8.40.0",
    "eslint-config-next": "13.4.2",
    "next": "13.4.3",
    "postcss": "8.4.23",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "tailwindcss": "3.3.2",
    "typescript": "5.0.4"
  },
  "devDependencies": {
    "cross-env": "^7.0.3",
    "npm-run-all": "^4.1.5"
  }
}
